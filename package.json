{
  "name": "ReactNativeBoilerplate",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "clean:android": "cd android && ./gradlew clean --warning-mode all && cd ..",
    "clean:ios": "cd ios && xcodebuild clean && cd ..",
    "android": "APP_ENV=dev react-native run-android",
    "android:apk": "cd android && ./gradlew assembleRelease  && cd ..",
    "android:aab": "cd android && ./gradlew bundleRelease && cd ..",
    "android:dev": "APP_ENV=dev npx react-native run-android",
    "android:dev-release": "APP_ENV=dev npx react-native build-android --mode=release --tasks assembleRelease",
    "android:prod": "APP_ENV=prod npx react-native run-android",
    "android:prod-release": "APP_ENV=prod npx react-native build-android --mode=release --tasks assembleRelease",
    "ios": "APP_ENV=dev react-native run-ios --simulator 'iPhone 14' --scheme 'ReactNativeBoilerplateDev'",
    "ios-device": "APP_ENV=dev react-native run-ios --device 'iPhone 12' --scheme 'ReactNativeBoilerplateDev'",
    "ios:dev": "APP_ENV=dev npx react-native run-ios --scheme 'ReactNativeBoilerplateDev'",
    "ios:dev-release": "APP_ENV=dev npx react-native build-ios --scheme 'ReactNativeBoilerplateDev' --mode 'Release'",
    "ios:prod": "APP_ENV=prod npx react-native run-ios --scheme 'ReactNativeBoilerplateProd'",
    "ios:prod-release": "APP_ENV=dev npx react-native build-ios --scheme 'ReactNativeBoilerplateProd' --mode 'Release'",
    "pod": "npx pod-install ios",
    "lint": "tsc --noEmit && eslint --fix ./src/**/*.{ts,tsx}",
    "pre-commit": "npm run format && npm run lint",
    "format": "prettier --write **/*.{js,ts,tsx,json} && prettier --check **/*.{js,ts,tsx,json}",
    "icons-update-plist": "npx rnvi-update-plist package.json ios/ReactNativeBoilerplate/Info.plist",
    "start": "react-native start",
    "start-reset-cache": "npm run start -- --reset-cache",
    "test": "jest",
    "logcat": "adb logcat *:S ReactNative:V ReactNativeJS:V",
    "xcode": "cd ios && xed . && cd ..",
    "dev-clean": "rm -rf ~/Library/Developer/Xcode/DerivedData",
    "app:asset": "npx react-native-asset",
    "swagger-generator": "npx @rtk-query/codegen-openapi openapi-config.json",
    "deeplink-add-ios": "npx uri-scheme add ReactNativeBoilerplate --ios",
    "deeplink-add-android": "npx uri-scheme add ReactNativeBoilerplate --android",
    "deeplink-open-ios": "npx uri-scheme open ReactNativeBoilerplate:// --ios",
    "deeplink-open-android": "npx uri-scheme open ReactNativeBoilerplate:// --android",
    "prepare": "husky install",
    "storybook:web": "start-storybook -p 6006 -c .storybook",
    "storybook:generate": "sb-rn-get-stories",
    "storybook:build": "build-storybook -c .storybook"
  },
  "dependencies": {
    "@bam.tech/react-native-image-resizer": "^3.0.11",
    "@notifee/react-native": "^9.1.8",
    "@react-native-community/netinfo": "^11.4.1",
    "@react-native-vector-icons/common": "^11.0.0",
    "@react-native-vector-icons/feather": "^4.29.2",
    "@react-native-vector-icons/fontawesome6": "^6.7.1",
    "@react-native-vector-icons/material-design-icons": "^7.4.47",
    "@react-navigation/bottom-tabs": "^7.2.0",
    "@react-navigation/native": "^7.0.14",
    "@react-navigation/native-stack": "^7.2.0",
    "@reduxjs/toolkit": "^2.5.0",
    "axios": "^1.7.9",
    "dayjs": "^1.11.13",
    "i18next": "^24.2.0",
    "md5": "^2.3.0",
    "react": "18.3.1",
    "react-hook-form": "^7.54.2",
    "react-i18next": "^15.2.0",
    "react-native": "0.77.0",
    "react-native-avoid-softinput": "^7.0.0",
    "react-native-calendars": "^1.1307.0",
    "react-native-config": "^1.5.3",
    "react-native-device-info": "^14.0.2",
    "react-native-dotenv": "^3.4.11",
    "react-native-gesture-handler": "^2.21.2",
    "react-native-image-picker": "^7.2.3",
    "react-native-keyboard-aware-scroll-view": "^0.9.5",
    "react-native-mmkv": "^3.2.0",
    "react-native-modal-datetime-picker": "^18.0.0",
    "react-native-permissions": "^5.2.1",
    "react-native-portalize": "^1.0.7",
    "react-native-reanimated": "^3.15.3",
    "react-native-render-html": "6.3.4",
    "react-native-safe-area-context": "^5.2.0",
    "react-native-screens": "4.6.0",
    "react-redux": "^9.1.2",
    "redux-persist": "^6.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/plugin-proposal-export-namespace-from": "^7.18.9",
    "@babel/preset-env": "^7.25.3",
    "@babel/preset-typescript": "^7.26.0",
    "@babel/runtime": "^7.25.0",
    "@eslint/js": "^9.7.0",
    "@react-native-async-storage/async-storage": "^1.24.0",
    "@react-native-community/cli": "15.0.1",
    "@react-native-community/cli-platform-android": "15.0.1",
    "@react-native-community/cli-platform-ios": "15.0.1",
    "@react-native-community/datetimepicker": "^8.2.0",
    "@react-native-community/slider": "^4.5.2",
    "@react-native/babel-preset": "0.76.5",
    "@react-native/eslint-config": "0.76.5",
    "@react-native/metro-config": "0.76.5",
    "@react-native/typescript-config": "0.76.5",
    "@storybook/addon-essentials": "^6.5.16",
    "@storybook/addon-ondevice-actions": "^7.6.20",
    "@storybook/addon-ondevice-controls": "^7.6.20",
    "@storybook/addon-react-native-web": "^0.0.24",
    "@storybook/builder-webpack5": "^6.5.16",
    "@storybook/manager-webpack5": "^6.5.16",
    "@storybook/react": "^6.5.16",
    "@storybook/react-docgen-typescript-plugin": "^1.0.1",
    "@storybook/react-native": "^7.6.20",
    "@types/jest": "^29.5.12",
    "@types/md5": "^2",
    "@types/node": "^18.14.1",
    "@types/react": "^19.0.2",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^7.13.1",
    "@typescript-eslint/parser": "^7.13.1",
    "babel-jest": "^29.7.0",
    "babel-loader": "^8.3.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "babel-plugin-react-native-web": "^0.19.12",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "eslint": "9.x",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-promise": "^6.2.0",
    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-sort": "^3.0.2",
    "eslint-plugin-unused-imports": "^4.0.0",
    "globals": "^15.8.0",
    "husky": "^9.1.1",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.7",
    "obfuscator-io-metro-plugin": "^2.1.3",
    "prettier": "3.3.3",
    "react-dom": "^18.3.1",
    "react-native-web": "^0.19.12",
    "react-test-renderer": "18.3.1",
    "typescript": "5.5.4",
    "typescript-eslint": "^7.17.0"
  },
  "engines": {
    "node": ">=18"
  }
}
